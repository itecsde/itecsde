.left_column {
	width: 60%;
	float:left;
}
.right_column {
	width:36%;
	float:right;
}



.element_field_box{
	position:relative;
	margin-top:20px;
	margin-bottom:10px;
	margin-left:5px;
	clear: both;
}

.field_body {
	min-height:10px;
	font-size:13px;	
}
.field_body i{
	bottom: 10px;
    position: absolute;
    right: 0px;
    z-index: -5;
}
.field_body:hover i{
	 z-index: 0;
}

.field_body:focus, .field_body textarea:focus, .field_body:hover input, .field_body:hover textarea{
	background-color: #F2F2F2;
}


.event_name, .person_name, .application_name, .lecture_name, .site_name, .documentary_name, .course_name, .article_name, .lre_name, .post_name, .slideshow_name, .biography_name{
	font-size:20px;
	font-weight:bold;
	font-family: "Arial";
	color:#3B3348;
	margin:10px 0px 15px 5px;
	overflow:auto;
	line-height: 23px;
	position:relative;		
}


#event_name, #person_name, #application_name, #group_name, #lecture_name, #site_name, #documentary_name, #course_name, #article_name, #lre_name, #post_name, #slideshow_name, #biography_name {
	font-size:20px;
	font-weight:bold;
	font-family: "Arial";
	color:#3B3348;
	line-height: 28px;
	height: 28px;
	float: left;
	width: 96%;
	margin: 5px 0 5px 5px;	
	resize: none;
}

.field_body.description textarea{
	border: none;
	-webkit-transition: height 0.2s;
	-moz-transition: height 0.2s;
	transition: height 0.2s;
	font-size: 13px;
	line-height: 20px;
	color: rgb(51, 51, 51);
	text-align: justify!important;
	margin-top: 0px;
	resize: none;
}


.event_description, .person_description, .application_description, .lecture_description, .site_description, .documentary_description, .course_description, .article_description, .lre_description, .post_description, .slideshow_description, .biography_description{
	text-align: justify;
	margin: 0 0 0 5px; 
	font-size:13px;	
	position:relative;
}

#event_description, #person_description, #application_description , #lecture_description, #site_description, #documentary_description, #course_description, #article_description, #lre_description, #post_description, #slideshow_description, #biography_description {
	width:97%;
}


.element_field_box .field_prompt{
	font-size:14px;
	font-weight:bold;
	font-family: "Arial";
	color:#3B3348;
}

.element_field_box .field_body span, .element_field_box .field_body input,.element_field_box .field_body textarea{
	font-size:13px;	
	font-family: "Arial";	
}

.event_tags {
	margin-top:10px;
}
.person_tags, .application_tags, .lecture_tags, .site_tags, .documentary_tags, .course_tags, .article_tags, .lre_tags {
	margin-top:30px;
}

.event_tags .field_body textarea, .person_tags .field_body textarea, .application_tags .field_body textarea, .lecture_tags .field_body textarea, 
.site_tags .field_body textarea, .documentary_tags .field_body textarea, .course_tags .field_body textarea, .article_tags, .lre_tags, .field_body textarea {
	resize: none;
	width:97%;
}

.event_data_range {
	margin-top:20px;
	margin-left:5px;
}

.event_data_range .start_date{
	float:left;
	width:45%;
}

.event_data_range .start_date input[type="text"] {
	width: 150px;
}
.event_data_range .end_date input[type="text"] {
	width: 150px;
}


.event_url .field_body input, .person_url .field_body input, .application_url .field_body input, .lecture_url .field_body input, .site_url .field_body input, 
.documentary_url .field_body input, .course_url .field_body input, .article_url, .lre_url, .field_body input {
	width:97%;
}

.element_field_box .subject_item {
	margin-top:5px;
}
.element_field_box .subject_item .delete_subject, .element_field_box .selector_with_slider .delete_selector_with_slider, 
.element_field_box .language_item .delete_other_language, .element_field_box .operating_system_item .delete_operating_system {
	margin-left:10px;
	font-family: "Arial";
	font-size:16px;
	font-weight:bold;
	color:#828387;
	cursor:pointer;
}

.element_field_box .add_item_to_itemize {
	margin-top:5px;
	font-family: "Arial";
	font-size:33px; 
	color: #828387;
	font-weight:bold;
	width: 25px;
}


.event_age_range .field_body{
	overflow:auto;	
}
.event_age_range .age_rage_field {
	float:left;
}

.event_age_range #event_age_range{
	width:60px;	
	
	
}

.event_age_range #slider-range {	
	width:250px;
	margin-left:40px;
	margin-top:5px;
}

.event_scraped_from, .person_scraped_from, .application_scraped_from{
	margin-top: 50px;
}




.element_field_box .selector_field, .element_field_box .slider_with_input,.element_field_box .slider_with_input .input_field, .element_field_box .delete_selector_with_slider {
	float:left;	
}


.person_expertises .slider_with_input .slider {	
	width:150px;
	margin-left:70px;
	margin-top:7px;
}

.application_functionalities .slider_with_input .slider {	
	width:120px;
	margin-left:70px;
	margin-top:7px;
}

.element_field_box .slider_with_input .input_field input{
	width:20px;
	margin-left:20px;
}
.element_field_box .slider_with_input .input_field span{
	margin-left:20px;
}

.element_field_box .slider_with_input .level {
	border: 0; 
	color: #f6931f; 
	font-weight: bold;
}

.element_field_box .language_item,  .element_field_box .selector_with_slider{
	margin-top:5px;
}



.right_column .element_field_box {
	margin-left:0px;
}
.right_column .element_image_box.picture_container {
	width:auto;
	max-width:95%;	
	width:95%;
	max-height:330px;
	min-height:120px;
	margin-right:0px;	
}

.event_address .field_body input, .person_address .field_body input, .site_address .field_body input{
	width: 92%;
}
.event_address.element_field_box. .person_address.element_field_box, .site_address.element_field_box {
	padding-top:10px;
}


.right_column .event_map , .right_column .person_map, .right_column .site_map{	
	width: 95%;
}




