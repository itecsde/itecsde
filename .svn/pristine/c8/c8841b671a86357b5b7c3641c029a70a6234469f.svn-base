/********************** RELATED ELEMENTS **********************/
/**************************************************************/
.related_elements_container {
	margin-top:40px;	
}
.related_element {
	margin-bottom:20px;
	padding:10px 0px 10px 20px;
	background: none repeat scroll 0% 0% #FBFBFB;
    overflow: hidden;
	-webkit-box-shadow: 0px 4px 10px rgba(50, 50, 51, 0.75);
	-moz-box-shadow:    0px 4px 10px rgba(50, 50, 51, 0.75);
	box-shadow:         0px 4px 10px rgba(50, 50, 51, 0.75);
}

.related_element .body {
	float:left;
}
.related_element .prompt {	
	color: #433A4A;
    font-size: 16px;
    font-weight: bold;
    margin-bottom:10px;  
}

.related_element .prompt a{	
    float:right;
    margin-right:20px; 
    cursor:pointer;     
    font-size: 15px;
}

.small_extract_box {	
	position: relative;
	float:left;
	margin-right:20px;
	width:245px;
	height:160px;		
	background-repeat: no-repeat;
	background-size: 100%;
	-webkit-background-size: cover;
  	-moz-background-size: cover;
  	-o-background-size: cover;
  	background-size: cover;
  	cursor:pointer;
}

.small_extract_box .small_extract_box_footer {
	position: absolute;
	width:245px;
	bottom: 0px;
	height: 40px;
	background: #463D52;
	-moz-opacity: 0.70;
	opacity: 0.70;
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha"(Opacity=70);	
	
}
.small_extract_box .small_extract_box_footer p {
	padding-right: 5px;
	text-align: right;
	font-size: 11px;
	padding-left:5px;	
	color: white; 
	font-weight: bold;	
	font-size:10px;	
}




.miniature_with_source {	
	position: relative;
	float:left;
	margin-right:20px;
	width:245px;
	height:170px;		
	background-repeat: no-repeat;
	background-size: 100%;
	-webkit-background-size: cover;
  	-moz-background-size: cover;
  	-o-background-size: cover;
  	background-size: cover;
  	cursor:pointer;
}

.miniature_with_source .miniature_with_source_header {
	position: absolute;
	width:245px;
	top: 0px;
	height: 40px;
	background: #463D52;
	-moz-opacity: 0.70;
	opacity: 0.70;
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha"(Opacity=70);
	overflow: hidden;	
	
	
}
.miniature_with_source .miniature_with_source_header p {
	padding-right: 5px;
	text-align: right;
	font-size: 11px;
	padding-left:5px;	
	color: white; 
	font-weight: bold;	
	font-size:10px;	
}
.miniature_with_source .miniature_with_source_footer {
	position: absolute;
	width:235px;
	bottom: 0px;
	padding:5px;
	height: 30px;
	background: rgba(70, 61, 82, 0.7);
	/*background: #463D52;*/
	/*-moz-opacity: 0.70;
	opacity: 0.70;
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha"(Opacity=70);*/
	overflow: hidden;	
	
}
.miniature_with_source .miniature_source_image {
	width:60px;
	height:30px;
	overflow:hidden;
	float:left;
	background: #FFFFFF;

	text-align:center;
}
.miniature_with_source .miniature_source_image img {

}

.miniature_with_source .miniature_text {
	float:left;
	width:165px;
	line-height:15px;
	padding-right: 5px;
	text-align: right;
	font-size: 11px;
	padding-left:5px;	
	color: white; 
	font-weight: bold;	
}
	




