.sources_container {	
	margin: 0 auto;
	margin-top:20px;
    position: relative;    
    width: 1140px;	
}


/************************************************/
/************* fin administration  **************/
/************************************************/

.extract_box_fixed{
	display: block;
	background-color: #FBFBFB;
	height: 420px;
	float:left;
	overflow: hidden;
	/*width: 18.8%;*/
	width:270px;
	margin: 0.5%;
	/*box-shadow: 3px 3px 2px 0 rgba(68, 68, 68, 0.5);*/
	border: 1px solid #9A9A9A;
	font-size: 14px;
	position:relative;
}

.extract_box_fixed .extract_box_fixed_image{
	height:185px;
	width:250px;
	background-repeat: no-repeat;
	background-size: 100%;
	-webkit-background-size: cover;
  	-moz-background-size: cover;
  	-o-background-size: cover;
  	background-size: cover;
}

.extract_box_button{
	cursor: pointer;
	padding: 10px;
}

.extract_box_fixed:hover{
	background-color: #F2F2F2;
}

.extract_box_fixed hr{
	color: #C6C6C6;
	background-color: #C6C6C6;
	height: 2px;
	border: 0;
	width: 98%;
	margin-top: 5px;
	margin-bottom: 5px;
	position: relative;
	left: 1%;
}

.extract_box_fixed span.extract_name {
	display:block;
	margin-bottom: 0px;
	word-wrap: break-word;
	
}

.extract_box_fixed .extract_description{
	text-align:justify;
	margin-top: 10px;
	z-index: 1;
	position: relative;
}

.extract_box_fixed .extract_description *{
	text-align:justify !important;
	margin-top: 10px !important;
	z-index: 1 !important;
	position: relative !important;
	font-family: "Trebuchet MS", Helvetica Neue, Helvetica, Arial, sans-serif !important;
	font-size: 14px !important;
	line-height: 20px !important;
	height: 200px;
	overflow: hidden;
	color: #333333 !important;
    background: transparent !important;

}

.extract_box_fixed span.extract_name{
	color: #000000;
	font-weight: bold;
	/*font-size: 24px;*/
	position: relative;
	line-height: 26px;
}



.extract_box_fixed .bookmark {	
	position:absolute;
	width:30px;
	height:30px;
	top:0px;
	right:0px;	
	z-index:2;
}

.extract_box_fixed .bookmark.bookmarked {
	background: url(/images/black_bookmarked_30.png);
}

.extract_box_fixed .bookmark.bookmarked:hover {
	background: url(/images/black_remove_bookmark_30.png);
}

.extract_box_fixed:hover .bookmark.add_bookmark  { 
	background: url(/images/black_add_bookmark_30.png);
}

.extract_box_fixed .extract_box_fixed_bottom {
	height: 50px;
	position:absolute;
	bottom:0px;
	width:100%;
}


.extract_box_fixed .extract_box_fixed_bottom img {
    border: 1px solid #868688;
    float: left;
    margin-right: 5px;
    top: 6px;
    vertical-align: middle;
}



/***********+ BOOKMARK EN SHOW *****************/
.show_header .bookmark {	
	position:absolute;
	width:50px;
	height:50px;
	top:0px;
	right:0px;	
	z-index:2;
	cursor:pointer;
}


.show_header .bookmark.bookmarked {
	background: url(/images/black_bookmarked_50.png);
}

.show_header .bookmark.bookmarked:hover {
	background: url(/images/black_remove_bookmark_50.png);
}

/*.show_header:hover .bookmark.add_bookmark  {*/
.show_header .bookmark.add_bookmark  { 
	background: url(/images/black_add_bookmark2_50.png);
}

.bookmark.add_bookmark:hover  { 
	background: url(/images/black_add_bookmark_50.png);
}







