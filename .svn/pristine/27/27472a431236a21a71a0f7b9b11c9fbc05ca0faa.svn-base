#element_header {
    background: none repeat scroll 0 0 #DBD2E5;
    margin: 0 0 0 0;
    overflow: visible;
	-webkit-box-shadow: 0px 3px 10px rgba(50, 50, 51, 0.75);
	-moz-box-shadow:    0px 3px 10px rgba(50, 50, 51, 0.75);
	box-shadow:         0px 3px 10px rgba(50, 50, 51, 0.75);	   
	height: 70px;
}

.ownership_box {
	width: 250px;
	border-right: 2px solid #D4CBDE;
	height: 50px;
	padding: 10px;
	display: table-cell;
	vertical-align: middle;
}

.dropdown_owner_selector {
	cursor: pointer;
	float: left;
	height:32px;
	margin-right: 10px;
	line-height: 32px;
	text-align:center;
}

.owner {
	float:left;
	margin-left:5px;
}

.ownership_box ul {
	  padding: 0;
	  position: absolute;
	  left: 0;
	  top: 70px;
	  width: inherit;
	  padding: inherit;
	  /*visibility: hidden;*/
	  -webkit-transiton: opacity 0.2s;
	  -moz-transition: opacity 0.2s;
	  -ms-transition: opacity 0.2s;
	  -o-transition: opacity 0.2s;
	  -transition: opacity 0.2s;	
	  margin: 0 0 0 0;
	  z-index: 999;
	  background: #DBD2E5; 	  
	  display: none;
	  opacity: 0;
	  visibility: hidden;	  
	-webkit-box-shadow: 0px 3px 10px rgba(50, 50, 51, 0.75);
	-moz-box-shadow:    0px 3px 10px rgba(50, 50, 51, 0.75);
	box-shadow:         0px 3px 10px rgba(50, 50, 51, 0.75);	  
}

.ownership_box ul li {
	  display: block; 
	  color: #534A5F;
      font-size: 13px;
      font-weight: bold;
	  
	  /*text-shadow: 0 -1px 0 #000;*/
	  padding: 3px;
	  cursor: pointer;
	  line-height: 36px;
}

.ownership_box ul li:hover {
	background: #666; 	
	color: #FFFFFF;
}

.ownership_box .visible{
  display: block;
  opacity: 1;
  visibility: visible;
}

