#web_title_container {
	width: 20%;
	float: left;
	height: 110px;
}

.web_title {
	display: table-cell;
	vertical-align: middle;
	height: 110px;
}

.web_title a{
	color: #FFFFFF;
	text-decoration: none;
	font-size: 48px;
}


#user_menu {
	height: 70px;
	width: 80%;
	left: 20%;
	float: left;
	text-align: right;
}

#user_menu div{
	padding:13px;
}


.nav_user{
	list-style:none;
	float: right;
}


/* Little tricking with positions */
.nav_user li {
	float:left;			/* Show list items inline */
	width:auto; 
	position:relative; 
}
	


/* Playing with Main Categories */
.nav_user li a {
	color:#fff;
	display:block;  	/* Making sure a element covers whole li area */
	padding:8px 7px 8px 7px; 
	text-decoration:none; /* No underline */
	text-align:center; 
}
		
/* Sub Cat Menu stuff*/
.nav_user  ul {
	background-color: #656565;	
	position:absolute; 
	left:0; 
	display:none; /* Hide it by default */
	margin:0 0 0 -2px; 
	padding:0; 
	list-style:none;
	z-index: 105;
	border: 1px solid #3E3E3E;
	box-shadow: 2px 2px 2px #6B6B6B;
}
		
.nav_user  ul li {
	width:260px; 
	float:left;
	border-top:none;
}
		
/* Sub Cat menu link properties */
.nav_user  ul a {
	display:block;    	/* Making sure a element covers whole li area */
	height:15px;
	padding:8px 7px 13px 7px;
	text-decoration:none;	
	border-top:none;
	border-bottom:1px solid #6B6B6B;
}
	

.nav_user li:hover {	
	font-weight: bold;
}

.nav_user ul li:hover {
	background-color: #9A9A9A;	
	
}



#nav_bar {
	position: relative;
	height: 40px;
	width: 80%;
	float:left;

}

#nav_bar ul {
    list-style-type: none;
    position: absolute;
    right: 0;
    bottom: 0;
    margin-right: 2px;
}


#nav_bar ul li{
	float: left;
    font-size: 17px;
	display:inline;
	padding: 8px 12px;
    margin-left: 2px;
	-moz-border-radius: 8px 8px 0 0;
	-webkit-border-radius: 8px 8px 0 0;
	border-radius: 8px 8px 0 0;
	-ms-border-radius: 8px 8px 0 0;
}

#nav_bar a{
	display: block;
	text-decoration: none;
	color: #FFFFFF;
	font-weight:bold;
	outline:0;
}

#section_header{
	
	height: 40px;
	width: 100%;
}

#content{
	background: none repeat scroll 0% 0% rgba(255,255,255,0.5);
	padding:166px 6px 6px 6px;
	column-count: 2;
    -webkit-column-count: 2;
    -moz-column-count: 2;
    -o-column-count: 2;
        /*Espacio entre columnas*/
    column-gap: 5px;
    -webkit-column-gap: 5px;
    -moz-column-gap: 5px;
    -o-column-gap: 5px;
    overflow: hidden;
    min-height: 600px;
}

.activity_box{
	display: inline-block;
	background-color: #FBFBFB;
	height: auto;
	padding: 15px;
	margin-bottom: 6px;
	float:inherit;
	min-width: 94%;
	/*para Firefox*/
	-moz-border-radius: 13px;
	/*para Safari y Chrome*/
	-webkit-border-radius: 13px;
	/* para Opera */
	border-radius: 13px;
	/* Para IE (no IE8)*/
	-ms-border-radius: 13px;
}

.activity_box hr{
	color: #C6C6C6;
	background-color: #C6C6C6;
	height: 2px;
	border: 0;
	width: 100%;
	margin-top: 5px;
	margin-bottom: 5px;
}

.activity_box span.activity_name {
	display:block;
	margin-bottom: 0px;
}

.activity_box p{
	text-align:justify;
	margin-top: 5px;
}

.a-left{
	float: left;
}

.a-right{
	float: right;
}

