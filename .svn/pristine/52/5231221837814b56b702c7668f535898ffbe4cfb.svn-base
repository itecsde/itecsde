	/*height: 50px;*/
	/*position: fixed;*/
	top: 0;
	width:100%;
	color: #ffffff;
	z-index: 5;
}

#header-up{
	height: 110px;
}

#bodyElementTop {
	position: relative;
    background: url("/images/topRight.gif") no-repeat fixed right top transparent;
    height: auto !important;
    min-height: 340px;
}

#bodyElementBottom {
    background: url("../images/bottomLeft.gif") no-repeat fixed left bottom transparent;
    height: auto !important;
    min-height: 100%;
}

/* Icons */
[class^="icon-"],
[class*=" icon-"] {
  display: inline-block;  width: 14px;
  height: 14px;
  margin-right: .4em;
  margin-left: .4em;
  line-height: 14px;
  vertical-align: text-top;
  background-image: url("/images/icons/glyphicons-halflings.png");
  background-position: 14px 14px;
  background-repeat: no-repeat;
  margin-top: 2px;
}

