<% if @num_results == 0 %>
	<div class="no_results"><%= t('no_results')%></div>			
<% end %>

<% @people.each do |person| %>
	<%= render :partial => "elements/element_fixed", :locals => {:element => person, :current_layout => current_layout} %>
<% end %>

<div class="clear"></div>

<div class= "paginate_controls active">
	<%= paginate @people %>
</div>