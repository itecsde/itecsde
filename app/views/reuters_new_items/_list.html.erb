<% if @num_results == 0 %>
	<div class="no_results"><%= t('no_results')%></div>			
<% end %>

<% @reutersnewitems.each do |reutersnewitem| %>
	<%= render :partial => "elements/element_fixed", :locals => {:element => reutersnewitem, :current_layout => current_layout} %>
<% end %>

<div class="clear"></div>

<div class= "paginate_controls active">
	<%= paginate @reutersnewitems %>
</div>