<% if @num_results == 0 %>
	<div class="no_results"><%= t('no_results')%></div>			
<% end %>

<% @biographies.each do |biography| %>
	<%= render :partial => "elements/element_fixed", :locals => {:element => biography, :current_layout => current_layout} %>
<% end %>

<div class="clear"></div>

<div class= "paginate_controls active">
	<%= paginate @biographies %>
</div>
